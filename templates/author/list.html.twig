<h1>List of authors<h1>
<table border="1">
<tr>
<th>id</th>
<th>username</th>
<th>email</th>
<th>action</th>
</tr>
{% for i in authors %}
<tr>
    <td>
    {{i.id}}
    </td>
    <td>
    {{i.username}}
    </td>
    <td>
    {{i.email}}
    </td>
    <td>
    <a href="{{ path('app4_author', { 'id': i.id }) }}">edit </a>
    <a href="{{ path('app5_author', { 'id': i.id }) }}">remove </a>
    </td>
</tr>
{% endfor %}
</table>